<paramCA>
    <fixed>
        <machine arch="christal" nbLine="10" nbCol="15"/>
        //default CA size and architecture
        <display iter="CAsize" CAwidth="650" nbCol="2"/>
        <simul t0="0" isPlaying="false"/>
        <inits>
            <layer init="center">llseed</layer>
            <layer init="border">tmp1</layer>
            <layer init="center">tmp2</layer>
            <layer init="def">defE</layer>
            <layer init="def">defF</layer>
            <layer init="def">defVe</layer>
        </inits>
        <sizes>
            <size nbCol="8" nbLine="6"/>
            <size nbCol="14" nbLine="12"/>
            <size nbCol="30" nbLine="12"/>
            <size nbCol="60" nbLine="12"/>
        </sizes>
        <layers>layers
            <layer>seed
                <field>llseed</field>
                <field>tmp1</field>
                <field>tmp2</field>
            </layer>
            <layer>def
                <field>defE</field>
                <field>defF</field>
                <field>defVe</field>
            </layer>
        </layers>
    </fixed>
    <expandedLayer>
        <Layer>
            layers
        </Layer>
        <Layer>
            def
        </Layer>
    </expandedLayer>
    <colorOfField>
        <layer color="0x0080FF">
            llseed
        </layer>
    </colorOfField>
</paramCA>