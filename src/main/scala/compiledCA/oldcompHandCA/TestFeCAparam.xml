<paramCA>
    <fixed>
        <machine arch="christal" nbLine="8" nbCol="6"/>
        //default CA size and architecture
        <display iter="none" CAwidth="1600" nbCol="1"/>
        <simul t0="0" isPlaying="false"/>
        <inits>
            <layer init="center">llseed</layer>
            <layer init="def">defE</layer>
            <layer init="def">defF</layer>
            <layer init="def">defVe</layer>
        </inits>
        <multiInit layer="llseed" inits="center,border,random,dottedBorder"/>
        <sizes>
            <size nbCol="8" nbLine="6"/>
            <size nbCol="14" nbLine="12"/>
            <size nbCol="30" nbLine="12"/>
            <size nbCol="60" nbLine="12"/>
        </sizes>
        <layers>seed
            <field>llseed</field>
            <field>defE</field>
            <field>defF</field>
            <field>defVe</field>
            <field>eF</field>
        </layers>
    </fixed>
    <expandedLayer>
        <Layer>
            seed
        </Layer>
    </expandedLayer>
    <colorOfField>
        <layer color="0x0080FF">
            llseed
        </layer>
        <layer color="0x0000FF">
            eF
        </layer>
    </colorOfField>
</paramCA>